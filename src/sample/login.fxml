    <TextField fx:id="username" layoutX="87.0" layoutY="156.0" prefHeight="25.0" prefWidth="216.0" promptText="User name" />
    <PasswordField fx:id="password" layoutX="87.0" layoutY="200.0" prefHeight="25.0" prefWidth="216.0" promptText="Password" />
    <Button fx:id="login" layoutX="251.0" layoutY="259.0" mnemonicParsing="false" onAction="#handleLogin" text="Log in" />
